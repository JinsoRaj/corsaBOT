{
  "name": "express-rest-es2017-boilerplate",
  "version": "1.7.0",
  "description": "Express boilerplate for building RESTful APIs",
  "author": "Daniel Sousa <sousa.dfs@gmail.com>",
  "main": "src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "precommit": "yarn lint",
    "start": "NODE_ENV=production node ./src/index.js",
    "dev": "NODE_ENV=production nodemon ./src/index.js",
    "command": "NODE_ENV=production node ./src/command.js",
    "lint": "eslint --ext .js,.json src",
    "lint:fix": "yarn lint -- --fix",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --timeout 20000 --recursive src/api/tests",
    "test:unit": "cross-env NODE_ENV=test mocha --require babel-register --compilers js:babel-core/register src/api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --require babel-register --compilers js:babel-core/register --timeout 20000 src/api/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --require babel-register --compilers js:babel-core/register --watch src/api/tests/unit",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "deploy": "sh ./deploy.sh",
    "docs": "apidoc -i src -o ./public/docs",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/index.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:danielfsousa/express-rest-es2017-boilerplate.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "passport",
    "es6",
    "es7",
    "es8",
    "es2017",
    "mocha",
    "istanbul",
    "nyc",
    "eslint",
    "Travis CI",
    "coveralls",
    "REST",
    "API",
    "boilerplate",
    "generator",
    "starter project"
  ],
  "dependencies": {
    "@postlight/mercury-parser": "^2.1.1",
    "bluebird": "^3.5.3",
    "co": "^4.6.0",
    "cors": "^2.8.5",
    "cron": "^1.7.0",
    "cross-env": "^5.2.0",
    "date-format": "^2.0.0",
    "dotenv-safe": "^6.1.0",
    "express": "^4.16.4",
    "express-rate-limit": "^3.4.0",
    "http-status": "^1.3.1",
    "https-proxy-agent": "^2.2.1",
    "jsbn": "^1.1.0",
    "node-fetch": "^2.3.0",
    "puppeteer": "^1.19.0",
    "socks-proxy-agent": "^4.0.2",
    "socks5-http-client": "^1.0.4",
    "telebot": "^1.2.6",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "bcryptjs": "2.4.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "redis-mock": "^0.44.0",
    "socks5-https-client": "^1.2.1",
    "supertest": "^4.0.2"
  }
}
